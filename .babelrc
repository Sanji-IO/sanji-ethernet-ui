{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "lodash",
    [
      "angularjs-annotate",
      {
        "explicitOnly": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "__coverage__",
          {
            "ignore": "*.+(test|spec).*"
          }
        ]
      ]
    }
  }
}